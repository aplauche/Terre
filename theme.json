{
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank"
		}
	],
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "comments",
			"title": "Comments"
		}
	],
	"settings": {
		"appearanceTools": true,
		"blocks": {
			"core/button": {
				"spacing": {
					"spacingSizes": [
						{
							"name": "XS",
							"size": "4px",
							"slug": "xs"
						},
						{
							"name": "SM",
							"size": "8px",
							"slug": "sm"
						},
						{
							"name": "MD",
							"size": "16px",
							"slug": "md"
						},
						{
							"name": "LG",
							"size": "20px",
							"slug": "lg"
						},
						{
							"name": "XL",
							"size": "32px",
							"slug": "xl"
						}
					]
				}
			}
		},
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"gradients": [				
				{
					"name": "Primary / Secondary",
					"slug": "primary-secondary-gradient",
					"gradient": "linear-gradient(135deg,var(--wp--preset--color--primary),var(--wp--preset--color--secondary))"
				}
			],
			"palette": [
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#A77B56"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#D9C0A2"
				},
				{
					"name": "Background",
					"slug": "bg",
					"color": "#fff"
				},
				{
					"name": "Background Secondary",
					"slug": "bg-secondary",
					"color": "#f0efeb"
				},
				{
					"name": "Main",
					"slug": "main",
					"color": "#4d4944"
				},
				{
					"name": "White",
					"slug": "white",
					"color": "#fff"
				},
				{
					"name": "Black",
					"slug": "black",
					"color": "#000"
				}
			]
		},
		"custom": {
			"fontWeight": {
				"thin": 100,
				"extra-light": 200,
				"light": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"extra-bold": 800,
				"black": 900
			},
			"lineHeight": {
				"body": 1.75,
				"heading": 1.1,
				"medium": 1.5,
				"one": 1
			},
			"spacing": {
				"base": "1rem"
			},
			"rounding":{
				"base": "16px"
			}
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1280px"
		},
		"spacing": {
			"spacingSizes": [
				{
					"name": "XS",
					"size": "20px",
					"slug": "xs"
				},
				{
					"name": "SM",
					"size": "clamp(24px, 4vw, 32px)",
					"slug": "sm"
				},
				{
					"name": "MD",
					"size": "clamp(32px, 6vw, 48px)",
					"slug": "md"
				},
				{
					"name": "LG",
					"size": "clamp(40px, 8vw, 64px)",
					"slug": "lg"
				},
				{
					"name": "XL",
					"size": "clamp(48px, 10vw, 120px)",
					"slug": "xl"
				},
				{
					"name": "2XL",
					"size": "clamp(60px, 12vw, 160px)",
					"slug": "2-xl"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell, \"Helvetica Neue\",sans-serif",
					"slug": "sans-serif",
					"name": "Sans Serif"
				},
				{
					"fontFamily": "\"Iowan Old Style\", \"Apple Garamond\", Baskerville, \"Times New Roman\", \"Droid Serif\", Times, \"Source Serif Pro\", serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\"",
					"slug": "serif",
					"name": "Serif"
				},
				{
					"fontFamily": "Menlo, Consolas, Monaco, \"Liberation Mono\", \"Lucida Console\", monospace",
					"slug": "mono",
					"name": "Monospace"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "xSmall",
					"size": "0.85rem",
					"slug": "xs"
				},
				{
					"fluid": false,
					"name": "Small",
					"size": "1rem",
					"slug": "sm"
				},
				{
					"fluid": {
						"min": "1.1rem",
						"max": "1.25rem"
					},
					"name": "Medium",
					"size": "1.25rem",
					"slug": "md"
				},
				{
					"fluid": {
						"min": "1.25rem",
						"max": "1.5rem"
					},
					"name": "Large",
					"size": "1.5rem",
					"slug": "lg"
				},
				{
					"fluid": {
						"min": "1.5rem",
						"max": "2rem"
					},
					"name": "xLarge",
					"size": "2rem",
					"slug": "xl"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "2.25rem"
					},
					"name": "2XL",
					"size": "2.25rem",
					"slug": "2-xl"
				},
				{
					"fluid": {
						"min": "2.25rem",
						"max": "3rem"
					},
					"name": "3XL",
					"size": "3rem",
					"slug": "3-xl"
				},
				{
					"fluid": {
						"min": "2.75rem",
						"max": "3.75rem"
					},
					"name": "4XL",
					"size": "3.75rem",
					"slug": "4-xl"
				},
				{
					"fluid": {
						"min": "3rem",
						"max": "4.5rem"
					},
					"name": "5XL",
					"size": "4.5rem",
					"slug": "5-xl"
				},
				{
					"fluid": {
						"min": "3.75rem",
						"max": "6rem"
					},
					"name": "6rem",
					"size": "6XL",
					"slug": "6-xl"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--bg)",
			"text": "var(--wp--preset--color--main)"
		},
		"spacing": {
			"blockGap": "calc(var(--wp--custom--spacing--base) * 2)",
			"padding": {
				"bottom": "0px",
				"left": "calc(var(--wp--custom--spacing--base) * 2)",
				"right": "calc(var(--wp--custom--spacing--base) * 2)",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans-serif)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "var(--wp--custom--font-weight--light)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "calc(var(--wp--custom--rounding--base) / 2)",
					"width": "2px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--white)"
				},
				"spacing": {
					"padding": {
						"top": "calc(var(--wp--custom--spacing--base) / 2)",
						"right": "var(--wp--custom--spacing--base)",
						"bottom": "calc(var(--wp--custom--spacing--base) / 2)",
						"left": "var(--wp--custom--spacing--base)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)",
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--main)"
				},
				"spacing": {
					"margin": {
						"top": "0.5rem",
						"bottom": "0px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xs)"
				}
			},
			"cite": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xs)",
					"fontStyle": "inherit"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--4-xl)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--3-xl)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
					
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--2-xl)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xl)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--lg)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--md)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"blocks": {
			"core/buttons": {
				"spacing": {
					"blockGap": "calc(var(--wp--custom--spacing--base) / 2)"
				}
			},
			"core/calendar": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			},
			"core/code": {
				"border": {
					"radius": "var(--wp--custom--rounding--base)",
					"width": "1px",
					"color": "var(--wp--preset--color--primary)"
				},
				"color": {
					"background": "var(--wp--preset--color--bg-secondary)",
					"text": "var(--wp--preset--color--main)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"bottom": "0px"
					}
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)",
					"lineHeight": "var(--wp--custom--line-height--medium)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			},
			"core/comments-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--lg)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--md)",
						"bottom": "var(--wp--custom--spacing--base)"
					}
				}
			},
			"core/post-comments-form": {
				"elements": {
					"h3": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--lg)"
						}
					}
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"top": "calc(var(--wp--custom--spacing--base) * 2)",
						"right": "calc(var(--wp--custom--spacing--base) * 2)",
						"bottom": "calc(var(--wp--custom--spacing--base) * 2)",
						"left": "calc(var(--wp--custom--spacing--base) * 2)"
					}
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"bottom": "calc(var(--wp--custom--spacing--base) * 1.25)",
						"top": "0px"
					}
				},
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"core/image": {
				"spacing": {
					"margin": "0"
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							}
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)",
					"textDecoration": "none"
				}
			},
			"core/paragraph": {
				"spacing": {
					"margin": "0"
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--3-xl)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"	
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"elements": {
					"link":{
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/preformatted": {
				"border": {
					"color": "var(--wp--preset--color--main)",
					"radius": "var(--wp--custom--rounding--base)",
					"width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--bg-secondary)",
					"text": "var(--wp--preset--color--main)"
				},
				"spacing": {
					"padding": {
						"top": "calc(var(--wp--custom--spacing--base) * 2)",
						"right": "calc(var(--wp--custom--spacing--base) * 2)",
						"bottom": "calc(var(--wp--custom--spacing--base) * 2)",
						"left": "calc(var(--wp--custom--spacing--base) * 2)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			},
			"core/pullquote": {
				"border": {
					"color": "var(--wp--preset--color--primary)",
					"style": "solid",
					"width": "2px",
					"radius": "var(--wp--custom--rounding--base)"
				},
				"spacing": {
					"padding": {
						"top": "calc(var(--wp--custom--spacing--base) * 2)",
						"right": "calc(var(--wp--custom--spacing--base) * 2)",
						"bottom": "calc(var(--wp--custom--spacing--base) * 2)",
						"left": "calc(var(--wp--custom--spacing--base) * 2)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--lg)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--preset--color--primary)",
					"style": "solid",
					"width": "2px",
					"radius": "var(--wp--custom--rounding--base)"
				},
				"spacing": {
					"margin": {
						"bottom": "calc(var(--wp--custom--spacing--base) * 2)",
						"left": "0",
						"right": "0",
						"top": "calc(var(--wp--custom--spacing--base) * 2)"
					},
					"padding": {
						"top": "calc(var(--wp--custom--spacing--base) * 2)",
						"right": "calc(var(--wp--custom--spacing--base) * 2)",
						"bottom": "calc(var(--wp--custom--spacing--base) * 2)",
						"left": "calc(var(--wp--custom--spacing--base) * 2)"
					}
				}
			},
			"core/separator": {
				"border": {
					"radius": "0",
					"width": "0"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--lg)"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--secondary)"
							}
						},
						"typography": {
							"textDecoration": "none",
							"fontWeight": "var(--wp--custom--font-weight--bold)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--lg)",
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "var(--wp--custom--line-height--one)"
				}
			},
			"core/social-links": {
				"spacing": {
					"blockGap": "12px"
				}
			},
			"core/spacer": {
				"spacing": {
					"margin": {
						"top": "0 !important"
					}
				}
			},
			"core/table": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0px"
					}
				}
			}
		}
	}
}